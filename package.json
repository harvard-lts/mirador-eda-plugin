{
  "name": "@harvard-lts/mirador-eda-plugin",
  "version": "0.1.1",
  "description": "A Mirador plugin for displaying Emily Dickinson Archive transcriptions with toggleable line breaks and editorial marks",
  "main": "dist/index.js",
  "source": "src/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "npm run serve",
    "clean": "rm -rf ./dist && rm -rf ./demo/dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "build": "run-p build:es",
    "build:es": "rollup -c",
    "serve": "webpack serve --mode development --open",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harvard-lts/mirador-eda-plugin.git"
  },
  "keywords": [
    "mirador",
    "iiif",
    "plugin",
    "emily-dickinson",
    "manuscript",
    "transcription",
    "scholarly-edition"
  ],
  "author": "Notch8",
  "license": "Apache-2.0",
  "homepage": "https://github.com/harvard-lts/mirador-eda-plugin#readme",
  "dependencies": {
    "jquery": "^3.7.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "mirador": "^3.0.0",
    "react-redux": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@reduxjs/toolkit": "^2.9.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.2",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "mirador": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "rollup": "^3.29.5",
    "url": "^0.11.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0"
  }
}
