{
  "name": "@harvard-lts/mirador-eda-plugin",
  "version": "0.1.5",
  "description": "A Mirador plugin for displaying Emily Dickinson Archive transcriptions with toggleable line breaks and editorial marks",
  "module": "dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run serve",
    "clean": "rm -rf ./dist && rm -rf ./demo/dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "build": "run-p build:es",
    "build:es": "rollup -c",
    "serve": "vite",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harvard-lts/mirador-eda-plugin.git"
  },
  "keywords": [
    "mirador",
    "iiif",
    "plugin",
    "emily-dickinson",
    "manuscript",
    "transcription",
    "scholarly-edition"
  ],
  "author": "Notch8",
  "license": "Apache-2.0",
  "homepage": "https://github.com/harvard-lts/mirador-eda-plugin#readme",
  "dependencies": {
    "jquery": "^3.7.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "mirador": "^3.0.0",
    "react-redux": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@reduxjs/toolkit": "^2.9.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^12.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "jsdom": "^27.4.0",
    "mirador": "^3.4.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "rollup": "^3.29.5",
    "url": "^0.11.4",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
